{
  "name": "discorcabot",
  "version": "1.0.0",
  "description": "node.js Discord bot",
  "main": "./dist/discorcabot.js",
  "author": "emyarod",
  "license": "MIT",
  "keywords": [
    "node.js",
    "discord",
    "bot"
  ],
  "bugs": {
    "url": "https://github.com/emyarod/discorcabot/issues"
  },
  "scripts": {
    "format": "prettier --write 'src/**/*.{json,md}'",
    "lint": "eslint src --ext .ts",
    "lint:fix": "yarn run lint --fix",
    "postinstall": "husky install",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && mkdir dist",
    "build": "webpack --mode=production --config webpack.config.babel.js",
    "dev": "webpack --mode=development --config webpack.config.babel.js && node dist/discorcabot",
    "start": "nodemon --watch src -e ts --verbose --exec npm run dev",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emyarod/discorcabot.git"
  },
  "homepage": "https://github.com/emyarod/discorcabot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "@discordjs/voice": "^0.6.0",
    "bufferutil": "^4.0.3",
    "cheerio": "^1.0.0-rc.10",
    "discord.js": "^13.1.0",
    "erlpack": "^0.1.3",
    "ffmpeg-static": "^4.4.0",
    "googleapis": "^84.0.0",
    "node-fetch": "^2.6.1",
    "sodium": "^3.0.2",
    "twitter-lite": "^1.1.0",
    "utf-8-validate": "^5.0.5",
    "uuid": "^8.3.2",
    "xmlbuilder2": "^3.0.2",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@types/node-fetch": "^2.5.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.24.1",
    "husky": ">=6",
    "jest": "^27.0.6",
    "lint-staged": ">=10",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
