{
  "name": "discorcabot",
  "version": "1.0.0",
  "description": "node.js Discord bot",
  "main": "./dist/discorcabot.js",
  "author": "emyarod",
  "license": "MIT",
  "keywords": [
    "node.js",
    "discord",
    "bot"
  ],
  "bugs": {
    "url": "https://github.com/emyarod/discorcabot/issues"
  },
  "scripts": {
    "lint": "tslint --project tsconfig.json -c tslint.json 'src/**/*.ts'",
    "clean": "rimraf dist",
    "prebuild": "npm run clean && mkdir dist",
    "build": "webpack --mode=production --config webpack.config.babel.js",
    "dev": "webpack --mode=development --config webpack.config.babel.js && node dist/discorcabot",
    "start": "nodemon --watch src -e ts --verbose --exec npm run dev",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emyarod/discorcabot.git"
  },
  "homepage": "https://github.com/emyarod/discorcabot#readme",
  "dependencies": {
    "@discordjs/uws": "^11.149.1",
    "bufferutil": "^4.0.1",
    "cheerio": "^1.0.0-rc.3",
    "discord.js": "github:discordjs/discord.js",
    "erlpack": "github:discordapp/erlpack",
    "ffmpeg-binaries": "^4.0.0",
    "googleapis": "^40.0.0",
    "libsodium-wrappers": "^0.7.4",
    "node-fetch": "^2.6.0",
    "node-opus": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@types/cheerio": "^0.22.11",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.8",
    "@types/node-fetch": "^2.3.7",
    "@types/uuid": "^3.4.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "tslint": "^5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2",
    "uuid": "^3.3.2",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
